<script>
$("#example5").sortable({
    connectWith: "#example5 ul",
    items: "li",
    placeholder: "ui-state-highlight",
    update : function(event, ui) {
       var order = $(this).sortable('toArray').toString();
       var serial = $(this).sortable('serialize');
       alert(order);
       alert(serial);

    }
 });
 </script>